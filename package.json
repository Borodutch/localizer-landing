{
  "name": "localizer-landing",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && rm -rf docs && mv dist docs && cp CNAME docs/CNAME",
    "distribute": "vue-cli-service serve --mode development",
    "update": "yarn build && git add . && git commit -m 'update' && git push"
  },
  "dependencies": {
    "@types/node": "^14.14.16",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.1.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.22.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vuetify": "^2.4.0",
    "vuex": "^3.6.0",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1",
    "vuex-persistedstate": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "typescript": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "prettier": "prettier/prettier#cd2b8a32b5ef77eb0047e7517b1ea22d605a094a",
    "@prettier/plugin-pug": "^1.10.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "prettier": {
    "pluginSearchDirs": [
      "."
    ],
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
